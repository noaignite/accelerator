{
  "name": "@noaignite/react-utils",
  "version": "0.10.0",
  "private": false,
  "description": "React utils",
  "keywords": [
    "react",
    "react-utils",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/noaignite/accelerator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/accelerator.git",
    "directory": "packages/react-utils"
  },
  "license": "MIT",
  "author": "NoA Ignite",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.mts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/ --ignore-pattern '**/*.test.*'",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@noaignite/utils": "workspace:*"
  },
  "devDependencies": {
    "@noaignite/types": "workspace:^",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.12",
    "esbuild-plugin-preserve-directives": "0.0.11",
    "jsdom": "^25.0.1",
    "react": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "5.7.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
