{
  "name": "@noaignite/react-utils",
  "version": "0.11.0",
  "private": false,
  "description": "React utils",
  "keywords": [
    "react",
    "react-utils",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/noaignite/accelerator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/accelerator.git",
    "directory": "packages/react-utils"
  },
  "license": "MIT",
  "author": "NoA Ignite",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.mts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.mts"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@noaignite/utils": "workspace:*"
  },
  "devDependencies": {
    "@noaignite/types": "workspace:^",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.7.9",
    "@types/react": "^19.0.2",
    "@vitest/coverage-v8": "^2.1.9",
    "esbuild-plugin-preserve-directives": "0.0.11",
    "eslint-config-react-hooks": "^1.0.0",
    "jsdom": "^25.0.1",
    "react": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.8.2",
    "vitest": "^2.1.9"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
