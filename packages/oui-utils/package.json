{
  "name": "@oakwood/oui-utils",
  "version": "0.0.2",
  "private": false,
  "author": "Martin Barri <maeertin@gmail.com>",
  "description": "Oakwood-UI Utils - Utility functions for Oakwood-UI.",
  "main": "./src/index.js",
  "keywords": [
    "react",
    "react-component",
    "material design",
    "material-ui",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oakwood/oui.git",
    "directory": "packages/oui-utils"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oakwood/oui/issues"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ../../babel.config.js ./src --out-dir ./build",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ../../babel.config.js ./src --out-dir ./build/esm",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish build --tag latest",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {},
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
