{
  "name": "@noaignite/create-app",
  "version": "0.1.0",
  "private": false,
  "author": "NoA Ignite",
  "description": "Create NoA Ignite react apps with one command",
  "keywords": [
    "react"
  ],
  "license": "MIT",
  "bin": {
    "create-app": "./index.js"
  },
  "files": [
    "./index.js",
    "./createApp.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/accelerator.git",
    "directory": "packages/create-app"
  },
  "bugs": {
    "url": "https://github.com/noaignite/accelerator/issues"
  },
  "scripts": {
    "build": "mkdir build && cp index.js build && cp createApp.js build && node ../../scripts/copy-files.js",
    "prebuild": "rimraf build",
    "release": "yarn build && npm publish ./build --tag latest",
    "release:yalc": "yarn build && yalc publish ./build"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^4.1.1",
    "cross-spawn": "^7.0.3",
    "validate-npm-package-name": "^3.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
