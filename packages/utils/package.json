{
  "name": "@noaignite/utils",
  "version": "3.5.1",
  "private": false,
  "description": "Utility functions used by NoA Ignite",
  "keywords": [
    "utils"
  ],
  "bugs": {
    "url": "https://github.com/noaignite/accelerator/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noaignite/accelerator.git",
    "directory": "packages/utils"
  },
  "license": "MIT",
  "author": "NoA Ignite",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test:types": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@noaignite/style-guide": "workspace:*",
    "@noaignite/types": "workspace:^",
    "@vitest/coverage-v8": "^2.1.9",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
